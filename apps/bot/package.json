{
  "name": "@elvia/bot",
  "version": "5.6.0",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsx ./scripts/clean.ts && pnpm run build:swc",
    "build:swc": "swc ./src --out-dir ./dist --copy-files --strip-leading-paths",
    "start": "node --no-warnings ./dist/index.js",
    "start:shard": "node --no-warnings ./dist/shard.js",
    "lint": "eslint \"src/**/*.ts\" --format=pretty",
    "lint:fix": "eslint \"src/**/*.ts\" --fix --format=pretty",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "typecheck": "tsc --noEmit",
    "register": "tsx --no-warnings ./scripts/register.ts"
  },
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rygent/Elvia.git",
    "directory": "apps/bot"
  },
  "peerDependencies": {
    "discord-api-types": "*",
    "express": "*",
    "http-errors": "*"
  },
  "dependencies": {
    "@elvia/database": "workspace:^",
    "@elvia/logger": "workspace:^",
    "@elvia/webserver": "workspace:^",
    "@iamtraction/google-translate": "^2.0.1",
    "@prisma/client": "^6.1.0",
    "@rygent/anilist": "^1.4.0",
    "@rygent/spotify": "^1.2.0",
    "@sapphire/discord-utilities": "^3.4.3",
    "@sapphire/time-utilities": "^1.7.14",
    "@sapphire/type": "^2.6.0",
    "@sapphire/utilities": "^3.18.1",
    "axios": "^1.7.9",
    "colorette": "^2.0.20",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "globby": "^14.0.2",
    "jiti": "^2.4.2",
    "moment": "^2.30.1",
    "moment-duration-format": "^2.3.2",
    "nanoid": "^5.0.9",
    "systeminformation": "^5.23.23",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@types/moment-duration-format": "^2.2.6",
    "@types/node": "^22.10.2",
    "commander": "^12.1.0",
    "eslint": "^9.17.0",
    "eslint-formatter-pretty": "^6.0.1",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9"
  },
  "engines": {
    "node": ">=v20.11.0"
  },
  "packageManager": "pnpm@9.15.1"
}
