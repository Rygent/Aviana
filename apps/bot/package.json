{
  "name": "@elvia/bot",
  "version": "6.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rimraf ./dist && yarn build:check && yarn build:swc",
    "build:check": "tsc --noEmit",
    "build:swc": "swc ./src --out-dir ./dist --copy-files --strip-leading-paths",
    "build:command": "tesseract register",
    "start": "node --no-warnings ./dist/index.js",
    "start:shard": "node --no-warnings ./dist/shard.js",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gihub.com/Rygent/Elvia.git",
    "directory": "apps/bot"
  },
  "dependencies": {
    "@elvia/database": "workspace:^",
    "@elvia/logger": "workspace:^",
    "@elvia/tesseract": "workspace:^",
    "@iamtraction/google-translate": "^2.0.1",
    "@prisma/client": "^5.21.1",
    "@rygent/anilist": "^1.4.0",
    "@rygent/spotify": "^1.1.1",
    "@sapphire/discord-utilities": "^3.4.1",
    "@sapphire/time-utilities": "^1.7.12",
    "@sapphire/type": "^2.5.1",
    "@sapphire/utilities": "^3.17.0",
    "axios": "^1.7.7",
    "colorette": "^2.0.20",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "moment": "^2.30.1",
    "moment-duration-format": "^2.3.2",
    "nanoid": "^5.0.7",
    "systeminformation": "^5.23.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.39",
    "@types/moment-duration-format": "^2.2.6",
    "@types/node": "^22.7.8",
    "eslint": "^9.13.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.4"
  },
  "engines": {
    "node": ">=v20.11.0"
  },
  "packageManager": "yarn@4.5.1"
}
