{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "#assets/*": ["./src/assets/*"],
      "#commands/*": ["./src/commands/*"],
      "#events/*": ["./src/events/*"],
      "#interactions/*": ["./src/interactions/*"],
      "#lib/*": ["./src/lib/*"],
      "#types/*": ["./src/types/*"]
    },
    "outDir": "./dist"
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["./dist/**/*"]
}
