{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "#assets/*": ["./src/assets/*"],
      "#commands/*": ["./src/commands/*"],
      "#events/*": ["./src/events/*"],
      "#interactions/*": ["./src/interactions/*"],
      "#lib/*": ["./src/lib/*"]
    },
    "outDir": "./dist"
  },
  "include": ["./src/**/*.ts", "./types/**/*.ts"],
  "exclude": ["./dist/**/*"]
}
