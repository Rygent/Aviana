  },
  "dependencies": {
    "@prisma/client": "^5.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.6.5",
    "eslint": "^8.50.0",
    "eslint-formatter-pretty": "^5.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.3.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "files": [
    "./dist/**/*.js*",
    "./dist/**/*.mjs*",
