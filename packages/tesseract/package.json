{
  "name": "@elvia/tesseract",
  "version": "1.0.0",
  "description": "The heart and soul of the Elvia bot.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "bin": "./bin/tesseract.js",
  "scripts": {
    "build": "tsup",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gihub.com/Rygent/Elvia.git",
    "directory": "packages/tesseract"
  },
  "dependencies": {
    "@elvia/logger": "workspace:^",
    "@sapphire/utilities": "^3.17.0",
    "commander": "^12.1.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "globby": "^14.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "files": [
    "./dist/"
  ],
  "engines": {
    "node": ">=v20.10.0"
  },
  "packageManager": "yarn@4.5.0"
}
