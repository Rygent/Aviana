{
  "$schema": "https://turborepo.org/schema.v1.json",
  "globalDependencies": ["tsconfig.json"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "outputMode": "errors-only"
    },
    "@elvia/bot#build": {
      "dependsOn": ["^build", "@elvia/database#db:generate"],
      "outputs": ["dist/**"],
      "outputMode": "errors-only"
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": [],
      "outputMode": "errors-only"
    },
    "format": {
      "dependsOn": ["^build"],
      "outputs": [],
      "outputMode": "errors-only"
    },
    "@elvia/database#db:generate": {
      "cache": false,
      "outputMode": "none"
    },
    "@elvia/database#db:push": {
      "cache": false,
      "outputMode": "none"
    }
  }
}
