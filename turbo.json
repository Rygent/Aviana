{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "outputLogs": "errors-only"
    },
    "@elvia/bot#build": {
      "dependsOn": ["^build", "@elvia/database#build"],
      "outputs": ["dist/**"],
      "outputLogs": "errors-only"
    },
    "@elvia/website#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", ".contentlayer/**"],
      "outputLogs": "errors-only"
    },
    "lint": {
      "outputLogs": "errors-only"
    },
    "format": {
      "outputLogs": "errors-only"
    },
    "typecheck": {
      "outputLogs": "errors-only"
    },
    "@elvia/database#db:generate": {
      "cache": false,
      "outputLogs": "none"
    },
    "@elvia/database#db:push": {
      "cache": false,
      "outputLogs": "none"
    }
  }
}
