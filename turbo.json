{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "outputMode": "errors-only"
    },
    "lint": {
      "cache": false,
      "outputMode": "errors-only"
    },
    "format": {
      "cache": false,
      "outputMode": "errors-only"
    }
  }
}
